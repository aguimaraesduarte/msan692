<script type="text/javascript" charset="utf-8">
  $(function () {
    if (!Cookies.get('disable_top_notification')) {
      showSiteTopNotification();
    }
    $('.js-dismiss-link').click(function (e) {
      e.preventDefault();
      Cookies.set('disable_top_notification', true, { expires: 365 });
      hideSiteTopNotification();
      ga('send', 'event', 'Notification Bar', 'Dismissed', 'Dismiss Book Notice');
    });
    function hideSiteTopNotification() {
      $('.site-top-notification-link').hide();
      $('.js-notification-spaceable').addClass('hide-spacer');
    }
    function showSiteTopNotification() {
      $('.site-top-notification-link').show();
      $('.js-notification-spaceable').removeClass('hide-spacer');
    }
  });
</script>
